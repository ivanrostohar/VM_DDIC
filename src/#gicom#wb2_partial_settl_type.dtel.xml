     <DDLANGUAGE>D</DDLANGUAGE>
     <DDTEXT>Konditionskontraktabrechnung: Teilabrechnungsart</DDTEXT>
     <REPTEXT>Teilabrechnungsart</REPTEXT>
     <SCRTEXT_S>Teilabrech</SCRTEXT_S>
