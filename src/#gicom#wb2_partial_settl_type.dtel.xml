    <item>
     <DOKIL>
      <ID>DE</ID>
      <OBJECT>/GICOM/WB2_PARTIAL_SETTL_TYPE</OBJECT>
      <LANGU>D</LANGU>
      <TYP>E</TYP>
      <VERSION>0004</VERSION>
      <TXTLINES>00029</TXTLINES>
      <DOKSTATE>A</DOKSTATE>
     </DOKIL>
     <HEAD>
      <TDOBJECT>DOKU</TDOBJECT>
      <TDNAME>/GICOM/WB2_PARTIAL_SETTL_TYPE</TDNAME>
      <TDID>DE</TDID>
      <TDSPRAS>D</TDSPRAS>
      <TDFORM>S_DOCU_SHOW</TDFORM>
      <TDSTYLE>S_DOCUS1</TDSTYLE>
      <TDVERSION>00004</TDVERSION>
      <TDLINESIZE>072</TDLINESIZE>
     </HEAD>
     <LINES>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;DEFINITION&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Legt fest, ob eine Teilabrechnung für den Kontrakt erlaubt ist.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;USE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Folgende Einstellungen sind möglich:</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>&lt;LS&gt;Keine Teilabrechnung&lt;/&gt;: Es ist keine Teilabrechnung erlaubt für den</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Kontrakt</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>&lt;LS&gt;Kumuliert&lt;/&gt;: Bei jeder neuen Teilabrechnung werden die</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>vorhergehenden Teilabrechnungen abgezogen</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>&lt;LS&gt;Getrennt&lt;/&gt;: Jede Teilabrechnung rechnet die Periode unabhängig ab</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Bei der Endabrechnung werden immer die vorhergehenden Teilabrechnungen</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>abgezogen</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;DEPENDENCIES&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;EXAMPLE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Es gibt eine Teilabrechnung pro Quartal und eine Endabrechnung am Ende</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>des Jahres. Die Staffel für die Abrechnung hat die folgenden Werte:</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>Ab 100.000: 1%</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>Ab 500.000: 2%</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>Ab 1.000.000: 3%</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Umsatz:</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>Quartal 1: 400,000</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>Quartal 2: 300,000</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>Quartal 3: 500,000</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>Quartal 4: 600,000</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Abrechnungswerte für kumulierte Teilabrechnungen</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>Quartal 1: 400,000 * 1% = 4,000</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>Quartal 2: 700,000 * 2 % = 14,000 - 4,000 = 10.000</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>Quartal 3: 1,200,000 * 3% = 36,000 - 14,000 = 22,000</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>Endabrechnung: 1,800,000 * 3% = 54,000 - 36,000 = 18,000</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Abrechnungswerte für separate Teilabrechnungen</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>Quartal 1: 400,000 * 1% = 4,000</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>Quartal 2: 300,000 * 1 % = 3,000</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>Quartal 3: 500,000 * 2% = 10,000</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>Endabrechnung: 1,800,000 * 3% = 54,000 - 17,000 = 37,000</TDLINE>
      </TLINE>
     </LINES>
    </item>
    <item>
     <DOKIL>
      <ID>DE</ID>
      <OBJECT>/GICOM/WB2_PARTIAL_SETTL_TYPE</OBJECT>
      <LANGU>E</LANGU>
      <TYP>E</TYP>
      <VERSION>0004</VERSION>
      <MASTERLANG>X</MASTERLANG>
      <TXTLINES>00029</TXTLINES>
      <DOKSTATE>A</DOKSTATE>
     </DOKIL>
     <HEAD>
      <TDOBJECT>DOKU</TDOBJECT>
      <TDNAME>/GICOM/WB2_PARTIAL_SETTL_TYPE</TDNAME>
      <TDID>DE</TDID>
      <TDSPRAS>E</TDSPRAS>
      <TDFORM>S_DOCU_SHOW</TDFORM>
      <TDSTYLE>S_DOCUS1</TDSTYLE>
      <TDVERSION>00004</TDVERSION>
      <TDLINESIZE>072</TDLINESIZE>
     </HEAD>
     <LINES>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;DEFINITION&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Specifies if a partial settlement is allowed for the contract.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;USE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>The following settings are possible:</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>&lt;LS&gt;No Partial Settlement&lt;/&gt;: No partial settlement is allowed for the</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>contract</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>&lt;LS&gt;Cumulative&lt;/&gt;: Each partial settlement offsets the preceding partial</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>settlements</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>&lt;LS&gt;Separate&lt;/&gt;: Each partial settlement settles the period</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>independently</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>The final settlement always offsets the preceding partial settlement</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;DEPENDENCIES&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;EXAMPLE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>There is a quarterly partial settlement and a final settlement at the</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>end of the year. The scale for the settlement has the following values:</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>From 100,000: 1%</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>From 500,000: 2%</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>From 1,000,000: 3%</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Business Volume:</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>Quarter 1: 400,000</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>Quarter 2: 300,000</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>Quarter 3: 500,000</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>Quarter 4: 600,000</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Settlement values for cumulative partial settlement</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>Quarter 1: 400,000 * 1% = 4,000</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>Quarter 2: 700,000 * 2 % = 14,000 - 4,000 = 10.000</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>Quarter 3: 1,200,000 * 3% = 36,000 - 14,000 = 22,000</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>Final Settlement: 1,800,000 * 3% = 54,000 - 36,000 = 18,000</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Settlement values for separate partial settlement</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>Quarter 1: 400,000 * 1% = 4,000</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>Quarter 2: 300,000 * 1 % = 3,000</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>Quarter 3: 500,000 * 2% = 10,000</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>B1</TDFORMAT>
       <TDLINE>Final Settlement: 1,800,000 * 3% = 54,000 - 17,000 = 37,000</TDLINE>
      </TLINE>
     </LINES>
    </item>
   </LONGTEXTS>
