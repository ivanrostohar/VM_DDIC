   <DD04_TEXTS>
    <item>
     <DDLANGUAGE>D</DDLANGUAGE>
     <DDTEXT>Konditionskontrakt oder Anspruchsberechtigter ist inaktiv</DDTEXT>
     <REPTEXT>Inaktiv</REPTEXT>
     <SCRTEXT_S>Inaktiv</SCRTEXT_S>
     <SCRTEXT_M>Inaktiv</SCRTEXT_M>
     <SCRTEXT_L>KK/AnspBer. inaktiv</SCRTEXT_L>
    </item>
   </DD04_TEXTS>
   <LONGTEXTS>
    <item>
     <DOKIL>
      <ID>DE</ID>
      <OBJECT>/GICOM/WCB_DEACTIVATED</OBJECT>
      <LANGU>D</LANGU>
      <TYP>E</TYP>
      <VERSION>0003</VERSION>
      <TXTLINES>00010</TXTLINES>
      <DOKSTATE>A</DOKSTATE>
     </DOKIL>
     <HEAD>
      <TDOBJECT>DOKU</TDOBJECT>
      <TDNAME>/GICOM/WCB_DEACTIVATED</TDNAME>
      <TDID>DE</TDID>
      <TDSPRAS>D</TDSPRAS>
      <TDFORM>S_DOCU_SHOW</TDFORM>
      <TDSTYLE>S_DOCUS1</TDSTYLE>
      <TDVERSION>00003</TDVERSION>
      <TDLINESIZE>072</TDLINESIZE>
     </HEAD>
     <LINES>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;DEFINITION&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Zeigt an, dass der Konditionskontrakt nicht aktiv ist oder, dass ein</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Anspruchsberechtigter nicht aktiv ist.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;USE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>In der Listenanzeige für Konditionskontrakte, zeigt das Kennzeichen</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>inaktive Konditionskontrakte an, d. h. Konditionskontrakte, die nicht</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>den Status &lt;LS&gt;Freigegeben&lt;/&gt; haben.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>In der Listenanzeige für Anspruchsberechtigte, zeigt das Kennzeichen</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>inaktive Anspruchsberechtigte an, die im Konditionskontrakt nicht</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>freigegeben wurden.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;DEPENDENCIES&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Falls ein Anspruchsberechtigter nicht freigegeben wurde, gelten für ihn</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>nicht die Konditionen des Konditionskontraktes.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;EXAMPLE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
     </LINES>
    </item>
    <item>
     <DOKIL>
      <ID>DE</ID>
      <OBJECT>/GICOM/WCB_DEACTIVATED</OBJECT>
      <LANGU>E</LANGU>
      <TYP>E</TYP>
      <VERSION>0003</VERSION>
      <MASTERLANG>X</MASTERLANG>
      <TXTLINES>00009</TXTLINES>
      <DOKSTATE>A</DOKSTATE>
     </DOKIL>
     <HEAD>
      <TDOBJECT>DOKU</TDOBJECT>
      <TDNAME>/GICOM/WCB_DEACTIVATED</TDNAME>
      <TDID>DE</TDID>
      <TDSPRAS>E</TDSPRAS>
      <TDFORM>S_DOCU_SHOW</TDFORM>
      <TDSTYLE>S_DOCUS1</TDSTYLE>
      <TDVERSION>00003</TDVERSION>
      <TDLINESIZE>072</TDLINESIZE>
     </HEAD>
     <LINES>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;DEFINITION&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Indicates that the condition contract is not active or that an eligible</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>partner is not active.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;USE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>In the list display of condition contracts the indicator denotes</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>inactive condition contracts, meaning condition contracts that do not</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>have status &lt;LS&gt;Released&lt;/&gt;.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>In the list display of eligible partners the indicator denotes inactive</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>eligible partners that have not been released in the condition contract.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;DEPENDENCIES&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>If an eligible partner has not been released, the conditions in the</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>condition contract do not apply to this eligible partner.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;EXAMPLE&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
      </TLINE>
     </LINES>
    </item>
   </LONGTEXTS>
  </asx:values>
