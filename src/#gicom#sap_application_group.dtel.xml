    <item>
     <DOKIL>
      <ID>DE</ID>
      <OBJECT>/GICOM/SAP_APPLICATION_GROUP</OBJECT>
      <LANGU>D</LANGU>
      <TYP>E</TYP>
      <VERSION>0008</VERSION>
      <TXTLINES>00011</TXTLINES>
      <DOKSTATE>A</DOKSTATE>
     </DOKIL>
     <HEAD>
      <TDOBJECT>DOKU</TDOBJECT>
      <TDNAME>/GICOM/SAP_APPLICATION_GROUP</TDNAME>
      <TDID>DE</TDID>
      <TDSPRAS>D</TDSPRAS>
      <TDFORM>S_DOCU_SHOW</TDFORM>
      <TDSTYLE>S_DOCUS1</TDSTYLE>
      <TDVERSION>00008</TDVERSION>
      <TDLINESIZE>072</TDLINESIZE>
     </HEAD>
     <LINES>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;DEFINITION&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Beschreibt eine Reihe von &lt;DS:DE.WSTATE&gt;Anwendungsstatus&lt;/&gt;, die ein</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Abrechnungsmanagementbeleg haben kann.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;DEPENDENCIES&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>/:</TDFORMAT>
       <TDLINE>IF &amp;[SWITCH]S4H_DOCU_ON_PREMISE&amp; = &apos;X&apos;.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Sie ordnen in der Customizing-Aktivität &lt;DS:SIMG.W_WLFA_0019&gt;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>=</TDFORMAT>
       <TDLINE>Statusgruppen mit Anwendungsstatus definieren&lt;/&gt; Anwendungsstatus zu</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Statusgruppen zu. Ein Applikationsstatus kann in mehreren Statusgruppen</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>enthalten sein.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Sie können Statusgruppen zu &lt;DS:DE.WRART&gt;Abrechnungsprozessarten&lt;/&gt; in</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>der Customizing-Aktivität &lt;DS:SIMG.W_WLFA_0010.SIMG_OLMR&gt;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>=</TDFORMAT>
       <TDLINE>Abrechnungsprozessarten festlegen&lt;/&gt; zuordnen.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>/:</TDFORMAT>
       <TDLINE>ENDIF.</TDLINE>
      </TLINE>
     </LINES>
    </item>
    <item>
