      <TDSTYLE>S_DOCUS1</TDSTYLE>
      <TDVERSION>00008</TDVERSION>
      <TDLINESIZE>072</TDLINESIZE>
     </HEAD>
     <LINES>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;DEFINITION&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Describe el conjunto de &lt;DS:DE.WSTATE&gt;estados de aplicación&lt;/&gt; que puede</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>tener un documento de gestión de liquidación.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;DEPENDENCIES&amp;</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>/:</TDFORMAT>
       <TDLINE>IF &amp;[SWITCH]S4H_DOCU_ON_PREMISE&amp; = &apos;X&apos;.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Los estados de aplicación se asignan a grupos de estados en la actividad</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>Customizing &lt;DS:SIMG.W_WLFA_0019&gt;Definir grupos de estados con estados</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>de aplicación&lt;/&gt;. Un estado de aplicación puede estar dentro de varios</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>grupos de estados.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Los grupos de estados se asignan a &lt;DS:DE.WRART&gt;clases de procesos de</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>liquidación&lt;/&gt; en la actividad Customizing</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>&lt;DS:SIMG.W_WLFA_0010.SIMG_OLMR&gt;Definir clases de procesos de liquidación</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>=</TDFORMAT>
       <TDLINE>&lt;/&gt;.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>/:</TDFORMAT>
       <TDLINE>ENDIF.</TDLINE>
      </TLINE>
     </LINES>
    </item>
