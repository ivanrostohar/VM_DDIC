     <HEAD>
      <TDOBJECT>DSYS</TDOBJECT>
      <TDNAME>SIMG/GICOM/GRP</TDNAME>
      <TDID>HY</TDID>
      <TDSPRAS>E</TDSPRAS>
      <TDFORM>S_DOCU_SHOW</TDFORM>
