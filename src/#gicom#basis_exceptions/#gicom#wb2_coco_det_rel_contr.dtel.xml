      <TLINE>
       <TDFORMAT>AS</TDFORMAT>
       <TDLINE>Controla si el contrato de condición es relevante para la determinación</TDLINE>
      </TLINE>
      <TLINE>
       <TDLINE>del contrato de condición en la determinación de precio.</TDLINE>
      </TLINE>
      <TLINE>
       <TDFORMAT>U1</TDFORMAT>
       <TDLINE>&amp;USE&amp;</TDLINE>
