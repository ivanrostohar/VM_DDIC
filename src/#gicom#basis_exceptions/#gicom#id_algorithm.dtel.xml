     <HEAD>
      <TDOBJECT>DOKU</TDOBJECT>
      <TDNAME>/GICOM/ID_ALGORITHM</TDNAME>
      <TDID>DE</TDID>
      <TDSPRAS>E</TDSPRAS>
      <TDFORM>S_DOCU_SHOW</TDFORM>
